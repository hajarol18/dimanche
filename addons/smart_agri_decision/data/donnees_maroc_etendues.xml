<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- ======================================== -->
        <!-- DONNÉES MAROCAINES ÉTENDUES -->
        <!-- ======================================== -->
        
        <!-- PARCELLES MAROCAINES -->
        <record id="parcelle_ble_1_maroc" model="smart_agri_parcelle">
            <field name="name">Parcelle Blé Principal - Maroc</field>
            <field name="code">PB001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="soil_type_id" ref="soil_limoneux_maroc"/>
            <field name="surface">50.0</field>
            <field name="latitude">33.972</field>
            <field name="longitude">-6.85</field>
            <field name="altitude">35</field>
            <field name="forme">rectangulaire</field>
            <field name="pente">2</field>
            <field name="exposition">sud</field>
            <field name="statut">active</field>
        </record>
        
        <!-- CULTURES MAROCAINES -->
        <record id="culture_ble_2024_maroc" model="smart_agri_culture">
            <field name="name">Blé d'Hiver 2024 - Maroc</field>
            <field name="code">BLE2024</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="parcelle_id" ref="parcelle_ble_1_maroc"/>
            <field name="type_culture">cereale</field>
            <field name="variete">Blé tendre d'hiver premium - Variété marocaine</field>
            <field name="date_semis" eval="time.strftime('%Y-10-15')"/>
            <field name="date_recolte" eval="time.strftime('%Y-07-20')"/>
            <field name="rendement_attendu">9.0</field>
            <field name="rendement_reel">8.8</field>
            <field name="statut">termine</field>
            <field name="notes">Année exceptionnelle malgré la sécheresse - Climat marocain</field>
        </record>
        
        <!-- INTERVENTIONS MAROCAINES -->
        <record id="intervention_semis_ble_maroc" model="smart_agri_intervention">
            <field name="name">Semis Blé d'Hiver - Maroc</field>
            <field name="code">INT-MAROC-001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="parcelle_id" ref="parcelle_ble_1_maroc"/>
            <field name="type_intervention">semis</field>
            <field name="date_intervention" eval="time.strftime('%Y-10-15')"/>
            <field name="cout_estime">1500.0</field>
            <field name="devise">MAD</field>
            <field name="description">Semis du blé d'hiver avec semoir de précision - Coût en Dirhams marocains</field>
            <field name="statut">termine</field>
        </record>
        
        <!-- INTRANTS MAROCAINS -->
        <record id="intrant_semences_ble_maroc" model="smart_agri_intrants">
            <field name="name">Semences Blé Premium - Maroc</field>
            <field name="code">SEM-MAROC-001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="type_intrant">semences</field>
            <field name="quantite">200.0</field>
            <field name="unite">kg</field>
            <field name="cout_unitaire">25.0</field>
            <field name="devise">MAD</field>
            <field name="cout_total">5000.0</field>
            <field name="description">Semences de blé d'hiver premium - Variété marocaine adaptée</field>
            <field name="statut">disponible</field>
        </record>
        
        <!-- UTILISATIONS D'INTRANTS MAROCAINES -->
        <record id="utilisation_semences_ble_maroc" model="smart_agri_utilisation_intrant">
            <field name="name">Utilisation Semences Blé - Maroc</field>
            <field name="code">UTIL-MAROC-001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="parcelle_id" ref="parcelle_ble_1_maroc"/>
            <field name="intrant_id" ref="intrant_semences_ble_maroc"/>
            <field name="quantite_utilisee">180.0</field>
            <field name="unite">kg</field>
            <field name="date_utilisation" eval="time.strftime('%Y-10-15')"/>
            <field name="cout_total">4500.0</field>
            <field name="devise">MAD</field>
            <field name="description">Utilisation des semences de blé pour le semis d'hiver - Coût en Dirhams</field>
        </record>
        
        <!-- MÉTÉO MAROCAINE -->
        <record id="meteo_rabat_maroc" model="smart_agri_meteo">
            <field name="name">Données Météo Rabat - Maroc</field>
            <field name="code">MET-MAROC-001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="date_mesure" eval="time.strftime('%Y-12-27')"/>
            <field name="temperature_min">8.5</field>
            <field name="temperature_max">18.2</field>
            <field name="temperature_moyenne">13.35</field>
            <field name="humidite">65</field>
            <field name="precipitations">12.5</field>
            <field name="ensoleillement">6.8</field>
            <field name="vent_vitesse">15.2</field>
            <field name="vent_direction">sud-ouest</field>
            <field name="description">Conditions météo typiques de décembre à Rabat, Maroc - Climat méditerranéen</field>
        </record>
        
        <!-- ALERTES CLIMATIQUES MAROCAINES -->
        <record id="alerte_secheresse_maroc" model="smart_agri_alerte_climatique">
            <field name="name">Alerte Sécheresse - Région Rabat-Salé - Maroc</field>
            <field name="code">ALT-MAROC-001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="type_alerte">secheresse</field>
            <field name="niveau">modere</field>
            <field name="date_alerte" eval="time.strftime('%Y-12-27')"/>
            <field name="description">Sécheresse modérée détectée dans la région de Rabat-Salé - Recommandations d'irrigation</field>
            <field name="recommandations">1. Augmenter la fréquence d'irrigation 2. Surveiller l'humidité du sol 3. Préparer les réserves d'eau</field>
            <field name="statut">active</field>
        </record>
        
        <!-- TENDANCES CLIMATIQUES MAROCAINES -->
        <record id="tendance_climatique_maroc" model="smart_agri_tendance_climatique">
            <field name="name">Tendance Climatique - Région Rabat - Maroc</field>
            <field name="code">TEND-MAROC-001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="periode_debut" eval="time.strftime('%Y-01-01')"/>
            <field name="periode_fin" eval="time.strftime('%Y-12-31')"/>
            <field name="type_tendance">rechauffement</field>
            <field name="intensite">moderee</field>
            <field name="description">Tendance au réchauffement modéré observée dans la région de Rabat - Impact sur les cultures céréalières</field>
            <field name="impact_cultures">Réduction de la période de croissance - Adaptation des variétés recommandée</field>
            <field name="statut">confirmee</field>
        </record>
        
        <!-- SCÉNARIOS CLIMATIQUES RCP MAROCAINS -->
        <record id="scenario_rcp_maroc" model="smart_agri_rcp_scenario">
            <field name="name">Scénario RCP 4.5 - Région Rabat - Maroc</field>
            <field name="code">RCP-MAROC-001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="scenario_rcp">rcp_4_5</field>
            <field name="horizon_temps">2050</field>
            <field name="description">Scénario RCP 4.5 pour la région de Rabat - Réchauffement modéré avec adaptation</field>
            <field name="impact_agricole">Augmentation de 2-3°C - Adaptation des variétés et des dates de semis</field>
            <field name="strategies_adaptation">1. Sélection de variétés résistantes 2. Modification des calendriers culturaux 3. Optimisation de l'irrigation</field>
            <field name="statut">actif</field>
        </record>
        
        <!-- ROTATIONS CULTURALES MAROCAINES -->
        <record id="rotation_culturelle_maroc" model="smart_agri_rotation_culturelle">
            <field name="name">Rotation Céréalière Marocaine - Rabat</field>
            <field name="code">ROT-MAROC-001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="description">Rotation céréalière optimisée pour le climat marocain - Blé, Orge, Maïs</field>
            <field name="cycle_rotation">3</field>
            <field name="annee_debut">2024</field>
            <field name="annee_fin">2027</field>
            <field name="avantages">1. Maintien de la fertilité du sol 2. Réduction des maladies 3. Optimisation des rendements</field>
            <field name="statut">active</field>
        </record>
        
        <!-- TABLEAUX DE BORD MAROCAINS -->
        <record id="tableau_bord_maroc" model="smart_agri_tableau_bord">
            <field name="name">Tableau de Bord - Ferme du Blé d'Or - Maroc</field>
            <field name="code">TB-MAROC-001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="description">Tableau de bord complet pour la gestion de l'exploitation céréalière marocaine</field>
            <field name="type_tableau">exploitation</field>
            <field name="periode">2024</field>
            <field name="indicateurs_cles">Rendement, Coûts, Météo, IA</field>
            <field name="statut">actif</field>
        </record>
        
        <!-- DASHBOARDS IA MAROCAINS -->
        <record id="dashboard_ia_maroc" model="smart_agri_ia_dashboard">
            <field name="name">Dashboard IA - SmartAgri Maroc</field>
            <field name="code">DASH-MAROC-001</field>
            <field name="exploitation_id" ref="exploitation_ferme_ble_maroc"/>
            <field name="description">Dashboard intelligent pour la gestion agricole basée sur l'IA - Contexte marocain</field>
            <field name="type_dashboard">ia_complet</field>
            <field name="modules_actifs">Prédictions, Optimisation, Alertes, Simulations</field>
            <field name="statut">actif</field>
        </record>
        
    </data>
</odoo>
