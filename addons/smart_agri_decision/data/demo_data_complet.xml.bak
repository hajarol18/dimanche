        <field name="code_scenario">RCP 4.5</field>
        <field name="type_scenario">rcp45</field>
        <field name="description">Scénario intermédiaire avec stabilisation des émissions - Réchauffement modéré 2.5°C</field>
        <field name="periode_debut" eval="(DateTime.today() - relativedelta(months=2)).strftime('%Y-%m-%d')"/>
        <field name="periode_fin" eval="(DateTime.today() + relativedelta(years=4)).strftime('%Y-%m-%d')"/>
        <field name="augmentation_temperature">2.5</field>
        <field name="variation_precipitation">-8</field>
        <field name="frequence_secheresse">18</field>
        <field name="intensite_events">28</field>
        <field name="exploitation_id" ref="exploitation_demo_meknes"/>
        <field name="state">valide</field>
    </record>

