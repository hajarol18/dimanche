    </record>

    <!-- SCÉNARIO CLIMATIQUE RCP 2.6 - OPTIMISTE -->
    <record id="scenario_rcp26_optimiste" model="smart_agri_scenario_climatique">
        <field name="name">Scénario RCP 2.6 - Optimiste</field>
        <field name="code_scenario">RCP 2.6</field>
        <field name="type_scenario">rcp26</field>
        <field name="description">Scénario optimiste avec limitation forte des émissions - Réchauffement limité à 1.5°C</field>
        <field name="periode_debut" eval="(DateTime.today() - relativedelta(months=3)).strftime('%Y-%m-%d')"/>
        <field name="periode_fin" eval="(DateTime.today() + relativedelta(years=3)).strftime('%Y-%m-%d')"/>
        <field name="augmentation_temperature">1.2</field>
        <field name="variation_precipitation">8</field>
        <field name="frequence_secheresse">5</field>
        <field name="intensite_events">12</field>
        <field name="exploitation_id" ref="exploitation_demo_meknes"/>
        <field name="state">valide</field>
    </record>

    <!-- SCÉNARIO CLIMATIQUE RCP 4.5 - INTERMÉDIAIRE -->
    <record id="scenario_rcp45_intermediaire" model="smart_agri_scenario_climatique">
