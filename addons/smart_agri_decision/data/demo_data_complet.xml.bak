<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- ======================================== -->
    <!-- DONNÉES DE DÉMONSTRATION COMPLÈTES -->
    <!-- Pour tester l'IA immédiatement ! -->
    <!-- ======================================== -->

    <!-- EXPLOITATION DÉMO 1 : MEKNÈS -->
    <record id="exploitation_demo_meknes" model="smart_agri_exploitation">
        <field name="name">Exploitation Meknès - Test IA</field>
        <field name="code">MEK001</field>
        <field name="type_exploitation">mixte</field>
        <field name="surface_totale">45.0</field>
        <field name="latitude">33.9716</field>
        <field name="longitude">-5.5471</field>
        <field name="altitude">500</field>
        <field name="zone_climatique">mediterraneen</field>
        <field name="type_sol">argileux</field>
        <field name="ph_sol">7.2</field>
        <field name="capacite_retention">180</field>
        <field name="description">Exploitation modèle pour tests IA - Céréales, légumineuses et arboriculture</field>
    </record>

    <!-- EXPLOITATION DÉMO 2 : AGADIR -->
    <record id="exploitation_demo_agadir" model="smart_agri_exploitation">
        <field name="name">Exploitation Agadir - Souss</field>
        <field name="code">AGA002</field>
        <field name="type_exploitation">arboriculture</field>
        <field name="surface_totale">32.0</field>
        <field name="latitude">30.4278</field>
        <field name="longitude">-9.5981</field>
        <field name="altitude">23</field>
        <field name="zone_climatique">aride</field>
