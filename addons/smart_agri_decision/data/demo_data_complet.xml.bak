        <field name="engrais_phosphore">0.0</field>
        <field name="engrais_potassium">0.0</field>
        <field name="scenario_optimiste">Rendement 4.5 t/ha avec conditions parfaites</field>
        <field name="scenario_realiste">Rendement 4.2 t/ha avec gestion optimale</field>
        <field name="scenario_pessimiste">Rendement 3.8 t/ha en cas de sécheresse</field>
        <field name="confiance">85</field>
        <field name="precision">82</field>
        <field name="score_ia">88</field>
        <field name="state">termine</field>
    </record>

