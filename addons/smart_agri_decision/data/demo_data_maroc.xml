        <!-- TYPES DE SOL MAROCAINS -->
        <record id="soil_tirs" model="smart_agri_soil_type">
            <field name="name">Sol Tirs</field>
            <field name="description">Sol argileux noir typique des plaines marocaines, excellente rétention d'eau</field>
            <field name="ph_min">7.0</field>
            <field name="ph_max">8.5</field>
            <field name="capacite_retention">85</field>
            <field name="drainage">Faible</field>
            <field name="richesse_nutriments">Très élevée</field>
        </record>
        
        <record id="soil_hamri" model="smart_agri_soil_type">
            <field name="name">Sol Hamri</field>
            <field name="description">Sol rouge méditerranéen, idéal pour l'arboriculture</field>
            <field name="ph_min">6.5</field>
            <field name="ph_max">7.5</field>
            <field name="capacite_retention">70</field>
            <field name="drainage">Bon</field>
            <field name="richesse_nutriments">Élevée</field>
        </record>
        
        <record id="soil_rmel" model="smart_agri_soil_type">
            <field name="name">Sol Rmel</field>
